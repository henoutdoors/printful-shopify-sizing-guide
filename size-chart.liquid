    var currentProduct = {{product | json}};
    var materialId = currentProduct.variants[0].option3;

    // special sauce for products that don't have colors
    if (_.isNull(materialId)) {
        if (_.isNumber(parseInt(currentProduct.variants[0].option2, 10))) {
            materialId = currentProduct.variants[0].option2;
        }
    }

    $(document).ready(function() {
      var units = 'inches';

      function init() {
        jQuery('#size-chart .content').html(product_sizes[materialId]);

        jQuery('#chart-type .US').on('click touchstart', function(event) {
          jQuery('#size-chart .content').empty().html(product_sizes[materialId]);
          units = 'inches';
          setClass();
        });

        jQuery('#chart-type .metric').on('click touchstart', function(event) {
          switchUnits('cm');
          units = 'cm';
          setClass();
        });

        setClass();
      }

      function switchUnits(targetUnits) {
          var i;
          var $table = $('#size-chart table');
          var tds = $table.find('tbody td').get();

          debugger;
          for (i in tds) {
              var td = tds[i];
              td.textContent = convertText(td.textContent, units, targetUnits);
          }

          var labels = $table.find('tbody td:first-child, tbody th:first-child').get();
          for (i in labels) {
              labels[i].textContent = labels[i].textContent.replace(/centimeters|cm|inches|inch/gi, targetUnits);
          }
      }

      function convertText(text, fromUnit, toUnit) {
          if (text.search('in') > 0 || text.search('cm') > 0) {
              return text;
          }

          text = text.replace(/\s/gi, '');
          text = $.trim(text);
          text = text.replace(/¼/gi, '.25');
          text = text.replace(/½/gi, '.5');
          text = text.replace(/¾/gi, '.75');
          text = text.replace(/1\/4/gi, '.25');
          text = text.replace(/1\/8/gi, '.125');
          text = text.replace(/1\/2/gi, '.5');
          text = text.replace(/3\/4/gi, '.75');
          text = text.replace(/5\/8/gi, '.625');
          text = text.replace(/7\/8/gi, '.875');

          if (parseInt(text, 10) == text) {
              return convertUnit(text, fromUnit, toUnit)
          }

          var parts = text.split('-');

          parts = parts.map(function (value) {
              var float = parseFloat(value);
              if (!float) {
                  return float;
              }
              return convertUnit(float, fromUnit, toUnit);
          });

          return parts.join(' - ');
      }

      function convertUnit(value, from, to) {
          if (from == 'cm' && to == 'inches') {
              return (value / 2.54).toFixed(1);
          }
          if (from == 'inches' && to == 'cm') {
              return (value * 2.54).toFixed(0);
          }
          return value;
      }

      function detectUnits() {
          var $table = $('#size-chart table');
          var $tr = $table.find('tbody tr:first-child').eq(0);
          var $firstCell = $tr.find('th:first-child').length ? $tr.find('th:first-child').eq(0) : $tr.find('td:first-child').eq(0);
          var firstRowText = $firstCell.text().toLowerCase();

          if (firstRowText.search('inch') > 0) {
              return 'inches';
          }
          if (firstRowText.search('cm') > 0 || firstRowText.search('centim') > 0) {
              return 'cm';
          }

          return false;
      }

      function setClass() {
        jQuery('#chart-type .selected').removeClass('selected');

        if (detectUnits() === 'inches') {
          jQuery('#chart-type .US').addClass('selected');
        }

        if (detectUnits() === 'cm') {
          jQuery('#chart-type .metric').addClass('selected');
        }
      }

      init();
    });
</script>

